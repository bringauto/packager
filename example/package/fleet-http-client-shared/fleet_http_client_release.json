{
  "Env": {},
  "DependsOn": [
    "zlib",
    "ba-logger",
    "boost",
    "fleet-protocol-interface",
    "fleet-protocol-cxx-helpers-static",
    "cpprestsdk"
  ],
  "Git": {
    "URI": "https://github.com/bringauto/fleet-protocol-http-client-cxx.git",
    "Revision": "v1.3.0"
  },
  "Build": {
    "CMake": {
      "Defines": {
        "CMAKE_BUILD_TYPE": "Release",
        "BRINGAUTO_INSTALL": "ON",
        "BRINGAUTO_PACKAGE": "ON",
        "BRINGAUTO_SYSTEM_DEP": "ON",
        "BRINGAUTO_SAMPLES": "OFF",
        "BRINGAUTO_TESTS": "OFF"
      }
    }
  },
  "Package": {
    "Name": "fleet-http-client-shared",
    "VersionTag": "v1.3.0",
    "PlatformString": {
      "Mode": "auto"
    },
    "IsLibrary": true,
    "IsDevLib": true,
    "IsDebug": false
  },
  "DockerMatrix": {
    "ImageNames": [
      "ubuntu2004",
      "ubuntu2204",
      "debian12",
      "debian11",
      "ubuntu2304",
      "ubuntu2310",
      "fedora40"
    ]
  }
}
